{
  "compilerOptions": {
    "target": "ES2020",
    "module": "commonjs",
    "lib": ["ES2020"],
    "outDir": "./dist",
    "rootDir": "./",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "resolveJsonModule": true,
    "types": ["node", "jest"],
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@types/*": ["src/types/*"],
      "@aiken/collection": ["src/core/packages/aiken-lang/stdlib/v2.2.0/lib/aiken/collection/index.ts"],
      "@aiken/crypto": ["src/core/packages/aiken-lang/stdlib/v2.2.0/lib/aiken/crypto.ts"],
      "@aiken/math": ["src/core/packages/aiken-lang/stdlib/v2.2.0/lib/aiken/math/index.ts"],
      "@aiken/cbor": ["src/core/packages/aiken-lang/stdlib/v2.2.0/lib/aiken/cbor/index.ts"],
      "@aiken/primitive": ["src/core/packages/aiken-lang/stdlib/v2.2.0/lib/aiken/primitive/index.ts"],
      "@aiken/interval": ["src/core/packages/aiken-lang/stdlib/v2.2.0/lib/aiken/interval.ts"],
      "@aiken/option": ["src/core/packages/aiken-lang/stdlib/v2.2.0/lib/aiken/option.ts"],
      "@aiken/rational": ["src/core/packages/aiken-lang/stdlib/v2.2.0/lib/aiken/math/rational.ts"],
      "@aiken/fuzz": ["src/core/packages/aiken-lang/fuzz/v2.2.0/lib/aiken/fuzz/index.ts"],
      "@aiken/prelude": ["src/core/packages/aiken-lang/prelude/v1.0.0/lib/aiken/index.ts"],
      "@aiken/merkle-patricia-forestry": ["src/core/packages/aiken-lang/merkle-patricia-forestry/v2.1.0/lib/aiken/merkle-patricia-forestry/index.ts"],
      "@aiken/helpers": ["src/core/packages/aiken-lang/merkle-patricia-forestry/v2.1.0/lib/aiken/merkle-patricia-forestry/helpers.ts"],
      "@aiken/merkling": ["src/core/packages/aiken-lang/merkle-patricia-forestry/v2.1.0/lib/aiken/merkle-patricia-forestry/merkling.ts"],
      "@cardano/address": ["src/core/packages/aiken-lang/stdlib/v2.2.0/lib/cardano/address/index.ts"],
      "@cardano/assets": ["src/core/packages/aiken-lang/stdlib/v2.2.0/lib/cardano/assets/index.ts"],
      "@cardano/transaction": ["src/core/packages/aiken-lang/stdlib/v2.2.0/lib/cardano/transaction/index.ts"],
      "@cardano/certificate": ["src/core/packages/aiken-lang/stdlib/v2.2.0/lib/cardano/certificate/index.ts"],
      "@cardano/governance": ["src/core/packages/aiken-lang/stdlib/v2.2.0/lib/cardano/governance/index.ts"],
      "@cardano/script_context": ["src/core/packages/aiken-lang/stdlib/v2.2.0/lib/cardano/script_context/index.ts"],
      "@cardano/credential": ["src/core/packages/aiken-lang/stdlib/v2.2.0/lib/cardano/address/credential/index.ts"],
      "@cardano/protocol_parameters": ["src/core/packages/aiken-lang/stdlib/v2.2.0/lib/cardano/governance/protocol_parameters/index.ts"],
      "@cardano/voter": ["src/core/packages/aiken-lang/stdlib/v2.2.0/lib/cardano/governance/voter/index.ts"],
      "@cardano/output_reference": ["src/core/packages/aiken-lang/stdlib/v2.2.0/lib/cardano/transaction/output_reference/index.ts"],
      "@cardano/script_purpose": ["src/core/packages/aiken-lang/stdlib/v2.2.0/lib/cardano/transaction/script_purpose/index.ts"],
      "@cardano/fuzz": ["src/core/packages/aiken-lang/fuzz/v2.2.0/lib/cardano/fuzz/index.ts"],
      "@aikscript/types": ["src/core/packages/aikscript/types/v1.0.0/lib/aikscript/index"]
    }
  },
  "include": ["src/**/*", "tests/**/*", "examples/**/*"],
  "exclude": ["node_modules", "dist"]
}
